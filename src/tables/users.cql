CREATE TABLE IF NOT EXISTS strafe.users (
    id TEXT,
    username TEXT,
    discriminator INT,
    global_name TEXT,
    avatar TEXT,
    bot BOOLEAN,
    system BOOLEAN,
    mfa_enabled BOOLEAN,
    banner TEXT,
    accent_color INT,
    locale TEXT,
    verified BOOLEAN,
    email TEXT,
    password TEXT,
    flags INT,
    premium_type INT,
    public_flags INT,
    avatar_decoration TEXT,
    created_at TIMESTAMP,
    edited_at TIMESTAMP,
    hide BOOLEAN,
    secret TEXT,
    last_pass_reset TIMESTAMP,
    dob TIMESTAMP,
    PRIMARY KEY((id), created_at)
) WITH CLUSTERING ORDER BY (created_at DESC);